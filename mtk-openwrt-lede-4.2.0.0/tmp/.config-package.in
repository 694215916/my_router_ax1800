	config PACKAGE_v4l-utils
		tristate "v4l-utils........................................ Video 4 Linux utilities"
		default y if DEFAULT_v4l-utils
		default m if ALL
		select PACKAGE_libv4l
		select PACKAGE_libc
		select PACKAGE_libssp if SSP_SUPPORT
		select PACKAGE_uclibcxx if USE_UCLIBCXX
