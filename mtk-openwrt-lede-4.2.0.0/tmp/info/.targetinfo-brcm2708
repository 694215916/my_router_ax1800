Source-Makefile: target/linux/brcm2708/Makefile
Target: brcm2708
Target-Board: brcm2708
Target-Name: Broadcom BCM27xx
Target-Arch: arm
Target-Arch-Packages: arm_xscale
Target-Features: audio display ext4 fpu gpio usb usbgadget
Target-Depends: 
Target-Optimization: -Os -pipe -mcpu=xscale
CPU-Type: xscale
Linux-Version: 4.4.198
Linux-Release: 1
Linux-Kernel-Arch: arm
Target-Description:
	Build firmware image for Broadcom BCM27xx SoC devices.
	Currently produces SD Card image for Raspberry Pi.
@@
Default-Packages: base-files libc libgcc busybox dropbear mtd uci opkg netifd fstools uclient-fetch logd dnsmasq iptables ip6tables ppp ppp-mod-pppoe firewall odhcpd odhcp6c brcm2708-gpu-fw kmod-usb-hid kmod-sound-core kmod-sound-arm-bcm2835 kmod-fs-vfat kmod-nls-cp437 kmod-nls-iso8859-1 
Target: brcm2708/bcm2708
Target-Board: brcm2708
Target-Name: BCM2708 based boards
Target-Arch: arm
Target-Arch-Packages: arm_arm1176jzf-s_vfp
Target-Features: arm_v6 audio display dt ext4 fpu gpio usb usbgadget
Target-Depends: 
Target-Optimization: -Os -pipe -mcpu=arm1176jzf-s -mfpu=vfp
CPU-Type: arm1176jzf-s+vfp
Linux-Version: 4.4.198
Linux-Release: 1
Linux-Kernel-Arch: arm
Target-Description:
	Build firmware image for Broadcom BCM2708 SoC devices.
@@
Default-Packages: base-files libc libgcc busybox dropbear mtd uci opkg netifd fstools uclient-fetch logd dnsmasq iptables ip6tables ppp ppp-mod-pppoe firewall odhcpd odhcp6c brcm2708-gpu-fw kmod-usb-hid kmod-sound-core kmod-sound-arm-bcm2835 kmod-fs-vfat kmod-nls-cp437 kmod-nls-iso8859-1 
Target-Profile: DEVICE_rpi
