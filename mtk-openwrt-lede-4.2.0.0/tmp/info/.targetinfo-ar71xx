Source-Makefile: target/linux/ar71xx/Makefile
Target: ar71xx
Target-Board: ar71xx
Target-Name: Atheros AR7xxx/AR9xxx
Target-Arch: mips
Target-Arch-Packages: mips_24kc
Target-Features: dt gpio mips16 pci usb
Target-Depends: 
Target-Optimization: -Os -pipe -mno-branch-likely -mips32r2 -mtune=24kc
CPU-Type: 24kc
Linux-Version: 4.4.198
Linux-Release: 1
Linux-Kernel-Arch: mips
Target-Description:

@@
Default-Packages: base-files libc libgcc busybox dropbear mtd uci opkg netifd fstools uclient-fetch logd dnsmasq iptables ip6tables ppp ppp-mod-pppoe firewall odhcpd odhcp6c kmod-gpio-button-hotplug swconfig kmod-ath9k wpad-mini uboot-envtools iwinfo
Target: ar71xx/generic
Target-Board: ar71xx
Target-Name: Generic
Target-Arch: mips
Target-Arch-Packages: mips_24kc
Target-Features: dt gpio mips16 pci squashfs usb
Target-Depends: 
Target-Optimization: -Os -pipe -mno-branch-likely -mips32r2 -mtune=24kc
CPU-Type: 24kc
Linux-Version: 4.4.198
Linux-Release: 1
Linux-Kernel-Arch: mips
Target-Description:
	Build firmware images for generic Atheros AR71xx/AR913x/AR934x based boards.
@@
Default-Packages: base-files libc libgcc busybox dropbear mtd uci opkg netifd fstools uclient-fetch logd dnsmasq iptables ip6tables ppp ppp-mod-pppoe firewall odhcpd odhcp6c kmod-gpio-button-hotplug swconfig kmod-ath9k wpad-mini uboot-envtools iwinfo
Target-Profile: Default
Target-Profile-Priority: 1
Target-Profile-Name: Default Profile (all drivers)
Target-Profile-Packages: kmod-usb-core kmod-usb-ohci kmod-usb2 kmod-usb-ledtrig-usbport 
Target-Profile-Description:
	Default package set compatible with most boards.
@@

